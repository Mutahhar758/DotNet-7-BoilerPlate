{
  "version": "2.0",
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "extensions": {
    "queues": {
      "batchSize": 16,
      "maxPollingInterval": "00:00:01",
      "maxDequeueCount": 1,
      "newBatchThreshold": 0
    }
  }
}